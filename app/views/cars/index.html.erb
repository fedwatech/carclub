<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Year</th>
      <th>Engine</th>
      <th>Power</th>
      <th>Price per day</th>
      <th>Description</th>
      <th>Seat</th>
      <th>Millage</th>
      <th>user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.name %></td>
        <td><%= car.year %></td>
        <td><%= car.engine %></td>
        <td><%= car.power %></td>
        <td><%= car.price_per_day %></td>
        <td><%= car.description %></td>
        <td><%= car.seat %></td>
        <td><%= car.millage %></td>
        <td><%= car.user.email %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
